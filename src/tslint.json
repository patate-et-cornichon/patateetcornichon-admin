{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "ordered-imports": [
          true,
          {
            "import-sources-order": "lowercase-first",
            "named-imports-order": "lowercase-first"
          }
        ]
    }
}
